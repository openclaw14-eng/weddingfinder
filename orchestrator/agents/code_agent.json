{
  "name": "Code Agent",
  "version": "1.0.0",
  "type": "code",
  "handler_type": "code",
  "description": "Agent for code generation, refactoring, and analysis tasks",
  "capabilities": [
    "code_generation",
    "code_refactoring",
    "code_review",
    "test_generation",
    "documentation",
    "bug_fixing"
  ],
  "config": {
    "language": "python",
    "style_guide": "pep8",
    "include_tests": true,
    "include_docs": true,
    "max_file_size_kb": 500,
    "auto_format": true
  },
  "supported_languages": [
    "python",
    "javascript",
    "typescript",
    "java",
    "go",
    "rust",
    "c",
    "cpp"
  ],
  "resources": {
    "max_memory_mb": 1024,
    "max_disk_mb": 50,
    "max_runtime_minutes": 30
  }
}
